{
  "name": "WebpackConfig-vue",
  "description": "webpack + vue配置",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./server/dev/server",
    "build": "npm run clean && cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.pro.js --progress",
    "clean": "./node_modules/.bin/rimraf build",
    "devdll": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --config ./config/webpack.dll.js --progress",
    "prodll": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --config ./config/webpack.dll.js --progress",
    "npmi": "npm install --production",
    "yarni": "yarn install --production=true",
    "server": "node ./server/pro/server",
    "eslint": "./node_modules/.bin/eslint src"
  },
  "dependencies": {
    "koa": "^2.5.0",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "koa-static-cache": "^5.1.2",
    "mime-types": "^2.1.18"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "@babel/polyfill": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/preset-flow": "^7.0.0-beta.46",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-component": "^1.1.0",
    "bundle-loader": "^0.5.6",
    "core-js": "^2.5.5",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "element-ui": "^2.3.6",
    "eslint": "^4.19.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack-middleware": "^1.0.7",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "optimize-css-assets-webpack-plugin": "^4.0.0",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-loader": "^14.2.2",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^3.0.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15"
  },
  "pre-commit": {
    "run": [
      "eslint"
    ]
  }
}
