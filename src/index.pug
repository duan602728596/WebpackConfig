- var imports = require('./imports');

doctype html
html
  head
    meta(charset='utf-8')
    meta(name='renderer' content='webkit')
    meta(name='viewport' content='width=device-width, initial-scale=1')
    link(rel='shortcut icon' href=require('./favicon.ico'))
    title Webpack App
  body
    div.react-app#react-app(data-app)
    if htmlWebpackPlugin.options.NODE_ENV === 'development'
      script(type='text/javascript' src=imports.dll)