{
  "name": "WebpackConfig-React",
  "description": "webpack + react配置",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development supervisor -w ./server,./config ./server/devServer.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./config/webpack.pro.js --progress",
    "clean": "rimraf ./build",
    "devdll": "cross-env NODE_ENV=development webpack --config ./config/webpack.dll.js --progress",
    "npmi": "npm install --production",
    "yarni": "yarn install --production=true",
    "server": "cross-env NODE_ENV=production node ./server/proServer.js",
    "eslint": "eslint ./src",
    "get-version": "node ./bin/get-version.js",
    "find": "node ./bin/find.js"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "koa": "^2.5.2",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
