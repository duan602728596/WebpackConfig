{
  "name": "WebpackConfig-Vue-Server",
  "description": "webpack + vue配置，服务器端渲染",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development HTTP_SERVER_PORT=5050 HTTPS_SERVER_PORT=5051 supervisor -w ./server,./config ./server/devServer.js",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config ./config/webpack.pro.js --progress",
    "start-server": "npm run clean-server && cross-env NODE_ENV=development webpack --config ./config-server/webpack.config.server.js --watch --watch-aggregate-timeout 1000",
    "build-server": "npm run clean-server && cross-env NODE_ENV=production webpack --config ./config-server/webpack.config.server.js --progress",
    "clean": "rimraf ./build",
    "clean-server": "rimraf ./build-server",
    "devdll": "cross-env NODE_ENV=development webpack --config ./config/webpack.dll.js --progress",
    "npmi": "npm install --production",
    "yarni": "yarn install --production=true",
    "server": "cross-env NODE_ENV=production HTTP_SERVER_PORT=5052 HTTPS_SERVER_PORT=5053 node ./server/proServer.js",
    "eslint": "eslint ./src",
    "get-version": "node ./bin/get-version.js",
    "find": "node ./bin/find.js"
  },
  "dependencies": {
    "cross-env": "^5.2.0",
    "koa": "^2.5.3",
    "koa-compress": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-router": "^7.4.0",
    "koa-static-cache": "^5.1.2",
    "mime-types": "^2.1.20"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@jnields/vue-helmet": "^3.0.0-rc.1",
    "autoprefixer": "^9.1.5",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.8.0",
    "classnames": "^2.2.6",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.0",
    "eslint": "^5.5.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "koa-webpack": "^5.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.2",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "supervisor": "^0.12.0",
    "terser-webpack-plugin": "^1.0.2",
    "url-loader": "^1.1.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "yargs": "^12.0.2"
  },
  "pre-commit": {
    "run": [
      "eslint"
    ]
  }
}
